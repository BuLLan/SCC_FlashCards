<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from FlashcardService WSDL service -->

  <service name="Flashcard" provider="java:RPC" style="document" use="encoded">
      <parameter name="wsdlTargetNamespace" value="http://flashcards"/>
      <parameter name="wsdlServiceElement" value="FlashcardService"/>
      <parameter name="wsdlServicePort" value="Flashcard"/>
      <parameter name="className" value="flashcards.FlashcardSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Flashcard"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>

  </service>
</deployment>
